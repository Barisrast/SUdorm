 />} />\n          <Route\n            path=\"dashboard\"\n            element={<PrivateRoute component={Dashboard} />}\n          />\n          <Route\n            path=\"create-profile\"\n            element={<PrivateRoute component={ProfileForm} />}\n          />\n          <Route\n            path=\"profile/:id\"\n            element={<PrivateRoute component={Profile} />}\n          />\n          <Route\n            path=\"profiles\"\n            element={<PrivateRoute component={Profiles} />}\n          />\n          <Route path=\"posts\" element={<PrivateRoute component={Posts} />} />\n          <Route path=\"posts/:id\" element={<PrivateRoute component={Post} />} />\n          <Route path=\"/*\" element={<NotFound />} />\n        </Routes>\n      </Router>\n    </Provider>\n  );\n};\n\nexport default App;\n","C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\store.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\components\\landing.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\components\\auth\\register.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\components\\auth\\login.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\actions\\types.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\components\\layout\\Alert.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\components\\dashboard\\Dashboard.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\actions\\auth.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\components\\layout\\NotFound.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\client\\src\\components\\profile\\Profile.js",[],"C:\\Users\\baris\\OneDrive\\Desktop\\git_projects\\new_sudorm_try\\cl