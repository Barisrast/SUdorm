\">\r\n                <label>Additional Preferences</label>\r\n                <input\r\n                  type=\"text\"\r\n                  name=\"additionalPreferences\"\r\n                  value={additionalPreferences}\r\n                  onChange={onChange}\r\n                ></input>\r\n              </div>\r\n              <hr></hr>\r\n              <div className=\"headphones-container\">\r\n                <label>Do You Wear Headphones While consuming media?</label>\r\n                <input\r\n                  type=\"radio\"\r\n                  name=\"Headphones-radio\"\r\n                  id=\"Headphones-yes\"\r\n                  value=\"Yes\"\r\n                  onChange={onChange}\r\n                ></input>\r\n                <span>Yes</span>\r\n                <input\r\n                  type=\"radio\"\r\n                  name=\"Headphones-radio\"\r\n                  id=\"Headphones-no\"\r\n                  value=\"No\"\r\n                  onChange={onChange}\r\n                ></input>\r\n                <span>No</span>\r\n              </div>\r\n              <hr></hr>\r\n              <div className=\"roommate-container\">\r\n                <label>Are You Looking For a Roommate?</label>\r\n                <input\r\n                  type=\"radio\"\r\n                  name=\"searchingRoommate\"\r\n                  value={true}\r\n                  onChange={onChange}\r\n                ></input>\r\n                <span>Yes</span>\r\n                <input\r\n                  type=\"radio\"\r\n                  name=\"searchingRoommate\"\r\n                  value={false}\r\n                  onChange={onChange}\r\n                ></input>\r\n                <span>No</span>\r\n              </div>\r\n              <hr></hr>\r\n              <div className=\"socialMedia-container\">\r\n                <div className=\"InstagramDiv\">\r\n                  <label>Instagram</label>\r\n                  <input\r\n                    t