inkedin: \"\",\n  youtube: \"\",\n  instagram: \"\",\n};\n\nconst ProfileForm = ({\n  profile: { profile, loading },\n  createProfile,\n  getCurrentProfile,\n}) => {\n  const [formData, setFormData] = useState(initialState);\n\n  const creatingProfile = useMatch(\"/create-profile\");\n\n  const navigate = useNavigate();\n\n  useEffect(() => {\n    // if there is no profile, attempt to fetch one\n    if (!profile) getCurrentProfile();\n\n    // if we finished loading and we do have a profile\n    // then build our profileData\n    if (!loading && profile) {\n      const profileData = { ...initialState };\n      for (const key in profile) {\n        if (key in profileData) profileData[key] = profile[key];\n      }\n\n      for (const key in profile.social) {\n        if (key in profileData) profileData[key] = profile.social[key];\n      }\n      // set local state with the profileData\n      setFormData(profileData);\n    }\n  }, [loading, getCurrentProfile, profile]);\n\n  const {\n    gender,\n    major,\n    classYear,\n    bio,\n    sleepRoutine,\n    socialSpectrum,\n    tidiness,\n    noiseTolerance,\n    hobbies,\n    spiritAnimal,\n    additionalPreferences,\n    searchingRoommate,\n    twitter,\n    facebook,\n    linkedin,\n    youtube,\n    instagram,\n  } = formData;\n\n  const onChange = (e) =>\n    setFormData({ ...formData, [e.target.name]: e.target.value });\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n    createProfile(formData, navigate, profile ? true : false);\n  };\n  return (\n    <div>\n      <SideBar />\n      <div className=\"matchForm\">\n        <div className=\"form-container\">\n          <form className=\"form\" onSubmit={onSubmit}>\n            <div className=\"innerForm-container\">\n              <div className=\"gender-container\">\n                <label>Gender:</label>\n                <div className=\"genderRadio-container\">\n                  <input\n                    type=\"radio\"\n                    name=\"gender\"\n                    value=\"male\"\n                    onChange={onChange}\n                  ></input>\n                  <span>Male</span>\n                  <input\n                    type=\"radio\"\n                    name=\"gender\"\n                    value=\"female\"\n                    onChange={onChange}\n                  ></input>\n                  <span>Female</span>\n                  <input\n                    type=\"radio\"\n                    name=\"gender\"\n                    value=\"other\"\n                    onChange={onChange}\n                  ></input>\n                  <span>Other</span>\n                  <input\n                    type=\"radio\"\n                    name=\"gender\"\n                    value=\"other\"\n                    onChange={onChange}\n                  ></input>\n                </div>\n              </div>\n              <hr></hr>\n              <div className=\"major-container\">\n                <label>Major:</label>\n                <input\n                  type=\"text\"\n                  name=\"major\"\n                  value={major}\n                  onChange={onChange}\n                ></input>\n              </div>\n              <hr></hr>\n              <div className=\"classYear-container\">\n                <label>Class Year:</label>\n                <input\n                  type=\"text\"\n                  name=\"classYear\"\n                  value={classYear}\n                  onChange={onChange}\n                ></input>\n              </div>\n              <hr></hr>\n              <div className=\"bio-container\">\n                <label>Bio:</label>\n                <input\n                  type=\"text\"\n                  name=\"bio\"\n                  value={bio}\n                  onChange={onChange}\n                ></input>\n              </div>\n              <hr></hr>\n              <div className=\"smoker-container\">\n                <label>Smoker:</label>\n                <input\n                  type=\"radio\"\n                  name=\"smokerBool\"\n                  value={true}\n                  onChange={onChange}\n                ></input>\n                <span>True</span>\n                <input\n                  type=\"radio\"\n                  name=\"smokerBool\"\n                  value={false}\n                  onChange={onChange}\n                ></input>\n                <span>False</span>\n              </div>\n              <hr></hr>\n              <div className=\"sleepRoutine-container\">\n                <label>Sleep Routine</label>\n                <input\n                  type=\"text\"\n                  name=\"sleepRoutine\"\n                  value={sleepRoutine}\n                  onChange={onChange}\n                ></input>\n              </div>\n              <hr></hr>\n              <hr></hr>\n              <div className=\"spiritAnimal-container\">\n                <label>Spirit Animal</label>\n                <input\n                  type=\"text\"\n                  name=\"spiritAnimal\"\n                  value={spiritAnimal}\n                  onChange={onChange}\n                ></input>\n              </div>\n              <hr></hr>\n              <div className=\"tidiness-container\">\n                <label>Tidiness:</label>\n                <input\n                  type=\"radio\"\n                  name=\"tidiness\"\n                  value=\"tidy\"\n                  onChange={onChange}\n                ></input>\n                <span>Tidy</span>\n                <input\n                  type=\"radio\"\n                  name=\"tidiness\"\n                  value=\"normal\"\n                  onChange={onChange}\n                ></input>\n                <span>Normal</span>\n                <input\n                  type=\"radio\"\n                  name=\"tidiness\"\n                  value=\"messy\"\n                  onChange={onChange}\n                ></input>\n                <span>Messy</span>\n              </div>\n              <hr></hr>\n\n              <div className=\"tidiness-container\">\n                <label>How social are you?:</label>\n                <input\n                  type=\"radio\"\n                  name=\"socialSpectrum\"\n                  value=\"Very Social\"\n                  onChange={onChange}\n                ></input>\n                <span>Very Social</span>\n                <input\n                  type=\"radio\"\n                  name=\"socialSpectrum\"\n                  value=\"normal\"\n                  onChange={onChange}\n                ></input>\n                <span>Normal</span>\n                <input\n                  type=\"radio\"\n                  name=\"socialSpectrum\"\n                  value=\"Unsocial\"\n                  onChange={onChange}\n                ></input>\n                <span>Unsocial</span>\n              </div>\n              <hr></hr>\n              <div className=\"noiseTolerance-container\">\n                <label>Noise Tolerance:</label>\n                <input\n                  type=\"radio\"\n                  name=\"noiseTolerance\"\n                  id=\"high-tolerance\"\n                  value=\"High\"\n                  onChange={onChange}\n                ></input>\n                <span>High</span>\n                <input\n                  type=\"radio\"\n                  name=\"noiseTolerance\"\n                  value=\"Normal\"\n                  onChange={onChange}\n                ></input>\n                <span>Normal</span>\n                <input\n                  type=\"radio\"\n                  name=\"noiseTolerance\"\n                  value=\"Low\"\n                  onChange={onChange}\n                ></input>\n                <span>Low</span>\n              </div>\n              <hr></hr>\n              <div className=\"hobbies-container\">\n                <label>Hobbies</label>\n                <input\n                  type=\"text\"\n                  name=\"hobbies\"\n                  value={hobbies}\n                  onChange={onChange}\n                ></input>\n              </div>\n              <hr></hr>\n              <div className=\"preferences-container\">\n                <label>Additional Preferences</label>\n                <input\n                  type=\"text\"\n                  name=\"additionalPreferences\"\n                  value={additionalPreferences}\n                  onChange={onChange}\n                ></input>\n              </div>\n              <hr></hr>\n              <div className=\"headphones-container\">\n                <label>Do You Wear Headphones While consuming media?</label>\n                <input\n                  type=\"radio\"\n                  name=\"Headphones-radio\"\n                  id=\"Headphones-yes\"\n                  value=\"Yes\"\n                  onChange={onChange}\n                ></input>\n                <span>Yes</span>\n                <input\n                  type=\"radio\"\n                  name=\"Headphones-radio\"\n                  id=\"Headphones-no\"\n                  value=\"No\"\n                  onChange={onChange}\n                ></input>\n                <span>No</span>\n              </div>\n              <hr></hr>\n              <div className=\"roommate-container\">\n                <label>Are You Looking For a Roommate?</label>\n                <input\n                  type=\"radio\"\n                  name=\"searchingRoommate\"\n                  value={true}\n                  onChange={onChange}\n                ></input>\n                <span>Yes</span>\n                <input\n                  type=\"radio\"\n                  name=\"searchingRoommate\"\n                  value={false}\n                  onChange={onChange}\n                ></input>\n                <span>No</span>\n              </div>\n              <hr></hr>\n              <div className=\"socialMedia-container\">\n                <div className=\"InstagramDiv\">\n                  <label>Instagram</label>\n                  <input\n                    type=\"text\"\n                    name=\"instagram\"\n                    value={instagram}\n                    onChange={onChange}\n                  ></input>\n                </div>\n\n                <div className=\"FacebookDiv\">\n                  <label>Facebook</label>\n                  <input\n                    type=\"text\"\n                    name=\"facebook\"\n                    value={facebook}\n                    onChange={onChange}\n                  ></input>\n                </div>\n                <div className=\"TwitterDiv\">\n                  <label>Twitter</label>\n                  <input\n                    type=\"text\"\n                    name=\"twitter\"\n                    value={twitter}\n                    onChange={onChange}\n                  ></input>\n                </div>\n                <div className=\"YoutubeDiv\">\n                  <label>Youtube</label>\n                  <input\n                    type=\"text\"\n                    name=\"youtube\"\n                    value={youtube}\n                    onChange={onChange}\n                  ></input>\n                </div>\n                <div className=\"LinkedinDiv\">\n                  <label>LinkedIn</label>\n                  <input\n                    type=\"text\"\n                    name=\"linkedin\"\n                    value={linkedin}\n                    onChange={onChange}\n                  ></input>\n                </div>\n              </div>\n              <hr></hr>\n              <button type=\"submit\" className=\"save-btn\" value=\"Save\">\n                Save\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  );\n};\nProfileForm.propTypes = {\n  createProfile: PropTypes.func.isRequired,\n  getCurrentProfile: PropTypes.func.isRequired,\n  profile: PropTypes.object.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  profile: state.profile,\n});\n\nexport default connect(mapStateToProps, { createProfile, getCurrentProfile })(\n  ProfileForm\n);\n","/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/routing/PrivateRoute.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/profiles/Profiles.js",["171"],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/posts/Posts.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/utils/setAuthToken.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/post/Post.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/utils/api.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/profile/ProfileAbout.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/profiles/ProfileItem.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/actions/alert.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/profile/ProfileTop.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/layout/SideBar.js",["172","173","174","175","176","177","178"],"import React, { useEffect } from \"react\";\nimport \"./SideBar.css\";\nimport { BrowserRouter, Routes, Route, Link } from \"react-router-dom\";\nimport { connect } from \"react-redux\";\nimport { getCurrentProfile } from \"../../actions/profile\";\nimport PropTypes from \"prop-types\";\nimport { logout } from \"../../actions/auth\";\n\nconst SideBar = ({\n  getCurrentProfile,\n  auth: { user, isAuthenticated },\n  logout,\n  profile: { profile },\n}) => {\n  useEffect(() => {\n    getCurrentProfile();\n  }, [getCurrentProfile]);\n\n  return (\n    <div className=\"sidebar\">\n      <div className=\"wrapper\">\n        <div className=\"sidebar\">\n          <div className=\"profile\">\n            <img src={user && user.avatar} />\n            <h3>{user && user.name}</h3>\n          </div>\n          <ul className=\"listSideBar\">\n            <li>\n              <Link to=\"/dashboard\">\n                <span className=\"item-nav\">Home</span>\n              </Link>\n            </li>\n\n            <li>\n              <Link to=\"/posts\">\n                <span className=\"item-nav\">Forum</span>\n              </Link>\n            </li>\n\n            <li>\n              <a href=\"#\">\n                <span className=\"item-nav\">Match</span>\n              </a>\n            </li>\n            <li>\n              <a href=\"#\">\n                <span className=\"item-nav\">Settings</span>\n              </a>\n            </li>\n            <li>\n              <a href=\"#\">\n                <span className=\"item-nav\">Filtered Search</span>\n              </a>\n            </li>\n            <li>\n              <Link to=\"/profiles\">\n                <span className=\"item-nav\">All Profiles</span>\n              </Link>\n            </li>\n            <li>\n              <a onClick={logout} href=\"#!\">\n                <i className=\"fas fa-sign-out-alt\" />{\" \"}\n                <span className=\"hide-sm\">Logout</span>\n              </a>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nSideBar.propTypes = {\n  getCurrentProfile: PropTypes.func.isRequired,\n  auth: PropTypes.object.isRequired,\n  profile: PropTypes.object.isRequired,\n};\nconst mapStateToProps = (state) => ({\n  auth: state.auth,\n  profile: state.profile,\n});\n\nexport default connect(mapStateToProps, { getCurrentProfile, logout })(SideBar);\n","/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/layout/Spinner.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/posts/PostItem.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/posts/PostForm.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/actions/profile.js",["179"],"import api from \"../utils/api\";\nimport { setAlert } from \"./alert\";\n\nimport {\n  GET_PROFILE,\n  GET_PROFILES,\n  PROFILE_ERROR,\n  UPDATE_PROFILE,\n  CLEAR_PROFILE,\n  ACCOUNT_DELETED,\n} from \"./types\";\n\n/*\n  NOTE: we don't need a config object for axios as the\n default headers in axios are already Content-Type: application/json\n also axios stringifies and parses JSON for you, so no need for \n JSON.stringify or JSON.parse\n*/\n\n// Get current user's profile\nexport const getCurrentProfile = () => async (dispatch) => {\n  try {\n    const res = await api.get(\"/profile/me\");\n\n    dispatch({\n      type: GET_PROFILE,\n      payload: res.data,\n    });\n  } catch (err) {\n    dispatch({\n      type: PROFILE_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n\n// Get all profiles\nexport const getProfiles = () => async (dispatch) => {\n  dispatch({ type: CLEAR_PROFILE });\n\n  try {\n    const res = await api.get(\"/profile\");\n\n    dispatch({\n      type: GET_PROFILES,\n      payload: res.data,\n    });\n  } catch (err) {\n    dispatch({\n      type: PROFILE_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n\n// Get profile by ID\nexport const getProfileById = (userId) => async (dispatch) => {\n  try {\n    const res = await api.get(`/profile/user/${userId}`);\n\n    dispatch({\n      type: GET_PROFILE,\n      payload: res.data,\n    });\n  } catch (err) {\n    dispatch({\n      type: PROFILE_ERROR,\n      payload: { msg: err.response.statusText, status: err.response.status },\n    });\n  }\n};\n\n// Create or update profile\nexport const createProfile =\n  (formData, navigate, edit = false) =>\n  async (dispatch) => {\n    try {\n      const res = await api.post(\"/profile\", formData);\n\n      dispatch({\n        type: GET_PROFILE,\n        payload: res.data,\n      });\n\n      dispatch(\n        setAlert(edit ? \"Profile Updated\" : \"Profile Created\", \"success\")\n      );\n\n      if (!edit) {\n        navigate(\"/dashboard\");\n      }\n    } catch (err) {\n      const errors = err.response.data.errors;\n\n      if (errors) {\n        errors.forEach((error) => dispatch(setAlert(error.msg, \"danger\")));\n      }\n\n      dispatch({\n        type: PROFILE_ERROR,\n        payload: { msg: err.response.statusText, status: err.response.status },\n      });\n    }\n  };\n\n// Delete account & profile\nexport const deleteAccount = () => async (dispatch) => {\n  if (window.confirm(\"Are you sure? This can NOT be undone!\")) {\n    try {\n      await api.delete(\"/profile\");\n\n      dispatch({ type: CLEAR_PROFILE });\n      dispatch({ type: ACCOUNT_DELETED });\n\n      dispatch(setAlert(\"Your account has been permanently deleted\"));\n    } catch (err) {\n      dispatch({\n        type: PROFILE_ERROR,\n        payload: { msg: err.response.statusText, status: err.response.status },\n      });\n    }\n  }\n};\n","/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/actions/post.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/post/CommentForm.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/post/CommentItem.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/reducers/index.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/utils/formatDate.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/reducers/profile.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/reducers/alert.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/reducers/auth.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/reducers/post.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/layout/alert.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/dashboard/dashboard.js",[],"/Users/omeryektasuzen/Desktop/sudorm/SUdorm/client/src/components/dashboard/Dashboard